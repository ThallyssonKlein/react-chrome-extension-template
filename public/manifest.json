{
  "manifest_version": 3,
  "name": "React Chrome Extension",
  "version": "1.0",
  "description": "React Chrome Extension Template",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo16.png",
      "48": "logo48.png",
      "128": "logo128.png"
    }
  },
  "permissions": ["activeTab"],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  }
}